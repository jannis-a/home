{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":dependencyDashboard",
    ":disableRateLimiting",
    ":enablePreCommit"
  ],
  "timezone": "Europe/Berlin",
  "commitBodyTable": true,
  "platformCommit": "enabled",
  "flux": {
    "fileMatch": [
      "(?:^|/)gotk-components\\.yaml$",
      "(?:^|/)flux\\.yaml$"
    ]
  }
}
